{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "filament/filament": "^v2.17.14",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^v10.3.2",
        "laravel/sanctum": "^v3.2.1",
        "laravel/tinker": "^v2.8.1",
        "league/commonmark": "^2.3",
        "livewire/livewire": "^v2.12.3",
        "spatie/laravel-medialibrary": "^10.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.8.1",
        "doctrine/dbal": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^12.9",
        "laravel/breeze": "^v1.19.2",
        "laravel/pint": "^v1.6.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.4",
        "nunomaduro/larastan": "^2.5",
        "pestphp/pest": "^v1.22.5",
        "pestphp/pest-plugin-laravel": "^v1.4.0",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
