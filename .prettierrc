{
  "tailwindConfig": "tailwind.config.js",
  "plugins": [
    "prettier-plugin-blade",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": [
        "*.blade.php"
      ],
      "options": {
        "parser": "blade"
      }
    }
  ],
  "singleQuote": true,
  "trailingComma": "all"
}
